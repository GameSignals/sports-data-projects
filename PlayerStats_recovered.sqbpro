</sql><sql name="SQL 2">SELECT
    team,
    SUM(goalsFor) AS GF_total,
    SUM(goalsAgainst) AS GA_total,
    CAST(SUM(goalsFor)*SUM(goalsFor) AS REAL) /
    (CAST(SUM(goalsFor)*SUM(goalsFor) AS REAL) + CAST(SUM(goalsAgainst)*SUM(goalsAgainst) AS REAL)) AS expected_wp
FROM all_teams
WHERE season = 2024
  AND situation = 'all'
GROUP BY team;
</sql><sql name="SQL 3*">SELECT

    ps.name,



    -- Raw on-ice goal differential

    SUM(ps.OnIce_F_goals - ps.OnIce_A_goals) AS goal_diff,



    -- Ice time in minutes

    SUM(ps.icetime) / 60.0 AS minutes_played,



    -- Goal Differential per 60 minutes

    (SUM(ps.OnIce_F_goals - ps.OnIce_A_goals) * 3600.0)

        / SUM(ps.icetime) AS gd_per_60,



    -- Salary

    s.Salary,



    -- Salary-adjusted value score (Moneyball number)

    ((SUM(ps.OnIce_F_goals - ps.OnIce_A_goals) * 3600.0)

        / SUM(ps.icetime)) / s.Salary AS value_score



FROM playerStats ps

JOIN PlayerSalaries s

    ON ps.name = s.Name



WHERE ps.situation = '5on5'

  AND ps.season = 2024



GROUP BY ps.name, s.Salary



-- Remove tiny-sample noise

HAVING minutes_played &gt;= 500



ORDER BY value_score DESC;

</sql><sql name="SQL 4"></sql><sql name="SQL 5*">SELECT

    ps.name,



    -- Expected on-ice goal differential

    SUM(ps.OnIce_F_xGoals - ps.OnIce_A_xGoals) AS xgoal_diff,



    -- Ice time in minutes

    SUM(ps.icetime) / 60.0 AS minutes_played,



    -- Expected Goal Differential per 60 minutes

    (SUM(ps.OnIce_F_xGoals - ps.OnIce_A_xGoals) * 3600.0)

        / SUM(ps.icetime) AS xgd_per_60,



    -- Salary

    s.Salary,



    -- Salary-adjusted expected value score

    ((SUM(ps.OnIce_F_xGoals - ps.OnIce_A_xGoals) * 3600.0)

        / SUM(ps.icetime)) / s.Salary AS xvalue_score



FROM playerStats ps

JOIN PlayerSalaries s

    ON ps.name = s.Name



WHERE ps.situation = '5on5'

  AND ps.season = 2024



GROUP BY ps.name, s.Salary



-- Remove tiny-sample noise

HAVING minutes_played &gt;= 500



ORDER BY xvalue_score DESC;

</sql><current_tab id="4"/></tab_sql></sqlb_project>

